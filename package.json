{
  "name": "rubyscore_sponsor_contracts",
  "version": "0.0.1",
  "description": "RubyScore sponsor contracts",
  "license": "MIT",
  "author": "AlexBalaban",
  "module": "module",
  "scripts": {
    "clean": "hardhat clean",
    "clean:deployments": "hardhat clean-deployments",
    "compile": "hardhat compile --force",
    "hh:node": "hardhat node",
    "test": "hardhat test ./test/index.test.ts",
    "test:watch": "hardhat watch test",
    "test:node": "hardhat test --network localhost ./test/index.test.ts",
    "coverage": "hardhat coverage",
    "gas-reporter": "ENABLE_GAS_REPORT=true hardhat test",
    "lint": "yarn lint:ts && yarn lint:sol",
    "lint:fix": "yarn lint:ts:fix && yarn lint:sol:fix",
    "lint:ts": "eslint --ext .ts .",
    "lint:ts:fix": "eslint --ext .ts . --fix",
    "lint:sol": "solhint -f table '{contracts,test}/**/*.sol' && prettier --check './contracts/**/*.sol'",
    "lint:sol:fix": "prettier --write './contracts/**/*.sol'",
    "migrate:mainnet": "hardhat deploy --network mainnet",
    "migrate:goerli": "hardhat deploy --network goerli",
    "migrate:scrollSepolia": "hardhat deploy --network scrollSepolia",
    "migrate:scrollMainnet": "hardhat deploy --network scrollMainnet",
    "migrate:polygon": "hardhat deploy --network polygon",
    "migrate:polygonMumbai": "hardhat deploy --network polygonMumbai",
    "migrate:baseMainnet": "hardhat deploy --network baseMainnet",
    "migrate:baseSepolia": "hardhat deploy --network baseSepolia",
    "migrate:bsc": "hardhat deploy --network bsc",
    "migrate:bscTestnet": "hardhat deploy --network bscTestnet",
    "migrate:mantle": "hardhat deploy --network mantleMainnet",
    "migrate:mantleTestnet": "hardhat deploy --network mantleTestnet",
    "migrate:mantaTestnet": "hardhat deploy --network mantaTestnet",
    "migrate:mantaMainnet": "hardhat deploy --network mantaMainnet",
    "migrate:taikoTestnet": "hardhat deploy --network taikoTestnet",
    "migrate:taikoMainnet": "hardhat deploy --network taikoMainnet",
    "migrate:optimismSepolia": "hardhat deploy --network optimismSepolia",
    "migrate:optimismMainnet": "hardhat deploy --network optimismMainnet",
    "migrate:berachainTestnet": "hardhat deploy --network berachainTestnet",
    "migrate:lineaTestnet": "hardhat deploy --network lineaTestnet",
    "migrate:lineaMainnet": "hardhat deploy --network lineaMainnet",
    "migrate:zoraMainnet": "hardhat deploy --network zoraMainnet",
    "migrate:zkEVMMainnet": "hardhat deploy --network zkEVMMainnet",
    "migrate:arbitrum": "hardhat deploy --network arbitrum",
    "verify:mainnet": "hardhat verify-by-name --network mainnet --contracts",
    "verify:goerli": "hardhat verify-by-name --network goerli --contracts",
    "verify:scrollSepolia": "hardhat verify-by-name --network scrollSepolia --contracts Rubyscore_Deposit",
    "verify:scrollMainnet": "hardhat verify-by-name --network scrollMainnet --contracts Rubyscore_Deposit",
    "verify:polygon": "hardhat verify-by-name --network polygon --contracts",
    "verify:polygonMumbai": "hardhat verify-by-name --network polygonMumbai --contracts",
    "verify:bsc": "hardhat verify-by-name --network bsc --contracts",
    "verify:mantle": "hardhat verify-by-name --network mantleMainnet --contracts",
    "verify:mantleTestnet": "hardhat verify-by-name --network mantleTestnet --contracts",
    "verify:mantaMainnet": "hardhat verify-by-name --network mantaMainnet --contracts Rubyscore_Deposit",
    "verify:baseMainnet": "hardhat verify-by-name --network baseMainnet --contracts Rubyscore_Deposit",
    "verify:baseSepolia": "hardhat verify-by-name --network baseSepolia --contracts Rubyscore_Deposit",
    "verify:taikoTestnet": "hardhat verify-by-name --network taikoTestnet --contracts Rubyscore_Deposit",
    "verify:taikoMainnet": "hardhat verify-by-name --network taikoMainnet --contracts Rubyscore_Deposit",
    "verify:berachainTestnet": "hardhat verify-by-name --network berachainTestnet --contracts Rubyscore_Deposit",
    "verify:optimismSepolia": "hardhat verify-by-name --network optimismSepolia --contracts Rubyscore_Deposit",
    "verify:optimismMainnet": "hardhat verify-by-name --network optimismMainnet --contracts Rubyscore_Deposit",
    "verify:lineaTestnet": "hardhat verify-by-name --network lineaTestnet --contracts Rubyscore_Deposit",
    "verify:lineaMainnet": "hardhat verify-by-name --network lineaMainnet --contracts Rubyscore_Deposit",
    "verify:zoraMainnet": "hardhat verify-by-name --network zoraMainnet --contracts Rubyscore_Deposit",
    "verify:zkEVMMainnet": "hardhat verify-by-name --network zkEVMMainnet --contracts Rubyscore_Deposit",
    "verify:arbitrum": "hardhat verify-by-name --network arbitrum --contracts Rubyscore_Deposit",
    "verify": "hardhat verify-by-name --network bscTestnet --contracts"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.4",
    "@nomicfoundation/hardhat-ethers": "^3.0.5",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.10",
    "@nomicfoundation/hardhat-toolbox": "^4.0.0",
    "@nomicfoundation/hardhat-verify": "^2.0.4",
    "@openzeppelin/contracts": "^5.0.1",
    "@openzeppelin/hardhat-upgrades": "^3.0.3",
    "@typechain/ethers-v6": "^0.5.1",
    "@typechain/hardhat": "^9.1.0",
    "@types/chai": "^4.3.11",
    "@types/mocha": "^10.0.6",
    "@types/node": ">=20.11.10",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "chai": "^4.4.1",
    "config": "^3.3.10",
    "dotenv": "^16.4.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "ethers": "^6.10.0",
    "hardhat": "^2.19.4",
    "hardhat-deploy": "^0.11.45",
    "hardhat-gas-reporter": "^1.0.9",
    "hardhat-watcher": "^2.5.0",
    "prettier": "^3.2.4",
    "prettier-plugin-solidity": "^1.3.1",
    "solhint": "^4.1.1",
    "solhint-plugin-prettier": "^0.1.0",
    "solidity-coverage": "^0.8.7",
    "solidity-docgen": "^0.6.0-beta.36",
    "ts-node": ">=10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typechain": "^8.3.2",
    "typescript": ">=5.3.3"
  }
}
